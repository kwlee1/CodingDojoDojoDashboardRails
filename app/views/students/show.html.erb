<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Student number </title>
    </head>
    <body>
        <h1><%= @dojo['branch'] %>'s <%= @me['first_name'] %> <%= @me['last_name'] %> </h1>
        <h3>Email: <%= @me['email'] %></h3>
        <h2>Cohort</h2>
        <table>
            <th><tr><td>First Name</td><td>Last Name</td><td>Email</td></tr></th>
            <% @students.each do |student| %>
            <tr>
                <td><%= student["first_name"] %></td>
                <td><%= student['last_name'] %></td>
                <td><%= student['email'] %></td>
            </tr>
            <% end %>
        </table>
        <a href="/">Home | </a><a href="/dojos/<%= @dojo['id'] %>/students/<%= @me['id'] %>/edit">Edit </a>
        <br>
    </body>
</html>
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>All the Dojos</title>
    </head>
    <body>
        <h1>Listing <%= @total %> Dojos</h1>
        <table>
            <th><tr>
                <td>Coding Dojo Branch</td>
                <td>Street</td>
                <td>City</td>
                <td>State</td>
            </tr></th>
            <% @dojos.each do |dojo| %>
            <tr><td><%= dojo['branch'] %></td><td><%= dojo['street'] %></td><td><%= dojo['city'] %></td><td><%= dojo['state'] %></td></tr>
            <% end %>
            
        </table>
        </div>
    </body>
</html>